.left-tri::before{
  position: absolute;
  content: '';
  width: 5px;
  height: 5px;
  border-style:solid;
  border-width:2px;
  border-color:#fff;
  border-radius:2px;
  background-color: #fff;
  transform:rotate(45deg);
  z-index: -1;
}

.right-tri::before{
  position: absolute;
  content: '';
  width: 5px;
  height: 5px;
  border-style:solid;
  border-width:2px;
  border-color:#CAD9F0;
  border-radius:2px;
  background-color: #CAD9F0;
  transform:rotate(45deg);
  z-index: -1;
}

